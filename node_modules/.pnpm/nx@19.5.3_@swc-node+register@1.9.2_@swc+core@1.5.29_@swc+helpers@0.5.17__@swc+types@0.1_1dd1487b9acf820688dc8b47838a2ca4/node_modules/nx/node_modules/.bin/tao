#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@nrwl+tao@19.5.3_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+ty_d57a53a6e6a664be2c1a0e201985a609/node_modules/@nrwl/tao/index.js" "$@"
else
  exec node  "$basedir/../../../../../@nrwl+tao@19.5.3_@swc-node+register@1.9.2_@swc+core@1.5.29_@swc+helpers@0.5.17__@swc+ty_d57a53a6e6a664be2c1a0e201985a609/node_modules/@nrwl/tao/index.js" "$@"
fi
